<div class="grid-row clearfix pagination">
    <div class="col-6">{{__ 'Page'}} <strong class="page">{{activePage}}</strong> {{__ 'of'}} <span class="total">{{totalPages}}</span></div>
    <div class="col-6 txt-rgt">
        <button class="btn-info small"><span class="icon-backward"></span>{{__ 'Previous'}}</button>
        <button class="btn-info small">{{__ 'Next'}}<span class="icon-forward r"></span></button>
    </div>
</div>
