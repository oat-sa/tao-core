{
  "baseUrl": "../js",
  "paths": {
    "text": "lib/text/text",
    "json": "lib/text/json",
    "css": "lib/require-css/css",
    "tpl": "tpl",
    "jquery": "../node_modules/jquery/jquery",
    "select2": "lib/select2/select2.min",
    "jquery.autocomplete": "lib/jquery.autocomplete/jquery.autocomplete",
    "jquery.tree": "lib/jsTree/jquery.tree",
    "jquery.cookie": "lib/jquery.cookie",
    "nouislider": "lib/sliders/jquery.nouislider",
    "jquery.fileDownload": "lib/jquery.fileDownload",
    "polyfill": "lib/polyfill",
    "url-polyfill": "../node_modules/url-polyfill/url-polyfill",
    "lodash": "../node_modules/lodash/lodash",
    "async": "lib/async",
    "moment": "../node_modules/moment/min/moment-with-locales",
    "handlebars": "../node_modules/handlebars/dist/handlebars",
    "class": "lib/class",
    "raphael": "lib/raphael/raphael",
    "scale.raphael": "lib/raphael/scale.raphael",
    "spin": "lib/spin.min",
    "pdfjs-dist/build/pdf": "lib/pdfjs/build/pdf",
    "pdfjs-dist/build/pdf.worker": "lib/pdfjs/build/pdf.worker",
    "mathJax": "../../../taoQtiItem/views/js/mathjax/MathJax",
    "ckeditor": "lib/ckeditor/ckeditor",
    "interact": "../../../tao/views/node_modules/interactjs/dist/interact",
    "d3": "lib/d3js/d3",
    "c3": "lib/c3js/c3",
    "css-builder": "lib/require-css/css-builder",
    "normalize": "lib/require-css/normalize",
    "i18ntr": "../locales/en-US",
    "router": "core/router",
    "babel-polyfill": "../../../tao/views/node_modules/@babel/polyfill/browser",
    "lib/uuid" : "../../../tao/views/node_modules/@oat-sa/tao-core-libs/dist/uuid",
    "core": "../../../tao/views/node_modules/@oat-sa/tao-core-sdk/dist/core",
    "util": "../../../tao/views/node_modules/@oat-sa/tao-core-sdk/dist/util",
    "ui": "../../../tao/views/node_modules/@oat-sa/tao-core-ui/dist"
  },

  "shim": {
    "moment": {
      "exports": "moment"
    },
    "handlebars" : {
      "exports": "Handlebars"
    },
    "ckeditor": {
      "exports": "CKEDITOR"
    },
    "ckeditor-jquery": ["ckeditor"],
    "class": {
      "exports": "Class"
    },
    "c3": {
      "deps": ["d3", "css!lib/c3js/c3.css"]
    },
    "lib/flatpickr/l10n/index": {
      "deps": ["lib/flatpickr/flatpickr"]
    }
  },
  "vendor": [
    "lib/require",
    "text",
    "json",
    "css",
    "tpl",
    "jquery",
    "jquery.cookie",
    "select2",
    "nouislider",
    "lodash",
    "async",
    "moment",
    "handlebars",
    "class",
    "raphael",
    "scale.raphael",
    "interact",
    "jquery.fileDownload",
    "jquery.autocomplete",
    "lib/farbtastic/farbtastic",
    "jquery.tree",
    "lib/jsTree/plugins/jquery.tree.checkbox",
    "d3",
    "c3",
    "core/**/*",
    "ui/**/*",
    "util/**/*",
    "uri",
    "context",
    "iframeResizer",
    "iframeNotifier",
    "babel-polyfill",
    "lib/uuid",
    "lib/moment-timezone.min"
  ],
  "exclude": [
    "json!i18ntr/messages.json",
    "mathJax",
    "ckeditor",
    "pdfjs-dist/build/pdf",
    "pdfjs-dist/build/pdf.worker",
    "normalize"
  ],
  "bootstrap": ["loader/bootstrap"],
  "default": [
    "controller/**/*",
    "component/**/*",
    "provider/**/*",
    "service/**/*"
  ]
}
